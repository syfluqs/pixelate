l=[0,1,8,15,22,23,24,25,18,11,4,3]
for i in range(len(l)):
	if i==0:
		print('s,m')
	else:
		if l[i+1]-l[i]==1 and l[i]-l[i-1]==1 or l[i+1]-l[i]==7 and l[i]-l[i-1]==7:
			print('s,m')
		elif l[i+1]-l[i]==1 and l[i]-[i-1]==7 or l[i+1]-l[i]==7 and l[i]-l[i-1]==-1:
			print('l,m')
		elif l[i+1]-l[i]==7 and l[i]-l[i-1]==1 or l[i+1]-l[i]==1 and l[i]-l[i-1]==-7:
			print('r,m')